<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            >

            <gradient
                android:angle="0"
                android:centerX="0.9"
                android:startColor="#FFA511"
                android:endColor="#FFA511"
                android:centerColor="#ffffff"
                android:type="linear"
                />
        </shape>
    </item>

    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            >

            <gradient
                android:angle="0"
                android:centerX="0.1"
                android:startColor="#FFA511"
                android:endColor="#00FFA511"
                android:centerColor="#ffffff"
                android:type="linear"
                />
        </shape>
    </item>





</layer-list>