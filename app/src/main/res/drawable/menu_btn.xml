<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android" >


    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            >
            <size
                android:width="140dp"
                android:height="140dp"
                />
            <solid
                android:color="@color/light_orange"
                />
            <stroke
                android:width="5dp"
                android:color="@color/dark_orange"
                />
            <corners
                android:radius="10dp"
                />
        </shape>
    </item>



    <!--three square dots-->
    <item
        android:left="20dp"
        android:right="100dp"
        android:top="20dp"
        android:bottom="100dp"
        >
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle"
            >

            <size
                android:width="20dp"
                android:height="20dp"
                />
            <solid
                android:color="@color/dark_orange"
                />
            <corners
                android:radius="1dp"
                />
        </shape>
    </item>

    <item
        android:left="20dp"
        android:right="100dp"
        android:top="60dp"
        android:bottom="60dp"
